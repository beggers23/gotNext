<div class='modal notShowing' id='areyousure'>
  <div class='box'>
    <p class='title is-1'> ARE YOU SURE YOU WANT TO STOP BALLIN?</p>
    <div class='buttons'>
      <a class='button is-danger' onclick='modals.deleteUser()' >Delete FOREVER</a>
      <a class='button is-success' onclick='modals.closeDeleteModal()' >Stick Around A Minute</a>
    </div>
  </div>
</div>



<div class='media social-template'>
  <figure class="media-left">
    <p class="image is-64x64">
      <img class='checkinPic'>
      <!-- Facebook Profile Image -->
    </p>
  </figure>
  <div class='media-content'>
    <div class='content'>
      <!-- Facebook Name -->
      <p>
        <strong onclick='modals.openOtherUsers( this )'>
          <span class='checkinUsername'></span>
          <span class='hidden checkinUserId'></span>
        </strong>
        <br>
        <span class='checkinTime'></span>
      </p>
      <a class='deleteCheckin checkinId' onclick='checkins.deleteCheckin(this)'>Delete</a>
      <!-- Parse this to a real time -->
    </div>
  </div>
</div>
